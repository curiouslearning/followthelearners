mixin consoleLog(title, content)
  .card.is-shadowless(style="height: 500px; margin: 5px;")
    .card-header-is-shadowless(style= "padding: 1px;")
      h6.card-header-title.is-size-6.is-centered #{title}
    .card-content(style="height: 85%; padding: 1px; overflow-y: scroll") 
      p.subtitle.is-size-6.has-text-grey-dark !{content}
mixin consoleCard(title, data, unit)
  .card.is-shadowless(style="margin: 5px;")
    .card-header.is-shadowless(style= "padding: 1px; display: flex; justify-content: center")
      h6.title.is-size-6.has-text-centered #{title}
    br
    .card-content(style="padding: 1px") 
      h3.title.is-size-3.has-text-grey-dark(style="text-align: center") !{data} !{unit}
      
