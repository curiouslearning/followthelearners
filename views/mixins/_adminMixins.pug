          span.is-icon.is-small
            i.fas.fa-external-link-alt(style="margin-left: .75rem")
  .tile.is-ancestor
    .tile
      .tile
        .tile.is-parent.is-vertical
          .tile.is-child.box
            +consoleCard("# of Jobs (Last 24 Hours)", 36)
          .tile.is-child.box
