            .tile.is-child.box
              +consoleCard("Cumulative Writes","0", "")
            .tile.is-child.box
              +consoleCard("Cumulative Deletes","0", "")
        .tile
          .tile.is-parent.is-vertical
            .tile.is-child.box
              +consoleCard("Peak Reads (1 minute)","75", "k")
            .tile.is-child.box
              +consoleCard("Peak Writes (1 minute)","0", "")
            .tile.is-child.box
              +consoleCard("Peak Deletes (1 minute)","0", "")
mixin stripe-console(data)
